            }
          }
        ]}

        var output = "<ul>";
        for (var i in data.users) {
          output+="<li>" + data.users[i].firstName + " " + data.users[i].lastName + "--" + data.users[i].joined.month + " " + data.users[i].joined.day + " " + data.users[i].joined.year + "</li>";
        }
        output+="</ul>";

        document.getElementById("placeholder").innerHTML=output;
    </script>

